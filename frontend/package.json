{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deadcode": "ts-prune --ignore 'src/vite-env.d.ts' --error",
    "deadcode-report": "ts-prune --ignore 'src/vite-env.d.ts' > .cleanup.frontend.tsprune.txt",
    "deps-check": "depcheck --ignores 'vite,@types/*,@vitejs/*,typescript-eslint'",
    "deps-report": "depcheck --ignores 'vite,@types/*,@vitejs/*,typescript-eslint' > .cleanup.frontend.depcheck.txt",
    "maintenance": "npm run deadcode-report && npm run deps-report"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "chart.js": "^4.5.0",
    "framer-motion": "^12.23.22",
    "lottie-react": "^2.4.1",
    "lottie-web": "^5.13.0",
    "lucide-react": "^0.546.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  }
}
